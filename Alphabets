
public class Alphabets{

public static void main(String[] args) {
   String input = "javas12java@";
    String output = "";

  for(int i = 0;  i< input.length(); i++) {
      char a = input.charAt(i);

    if(Character.isLowerCase(a)) {
     output += a;

      }
  }

       System.out.println(output);
}

}
